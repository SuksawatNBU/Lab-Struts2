<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="action-support" namespace="/jsp/action" extends="struts-default">
	
		<action name="*POJOAction" class="com.cct.labstruts2.web.actionsupport.action.LoginPOJOAction" method="{1}">
			<result name="login">/jsp/lab/actionsupport/action/login.jsp</result>
			<result name="success">/jsp/lab/actionsupport/action/loginSuccess.jsp</result>
			<result name="error">/jsp/lab/actionsupport/action/loginFail.jsp</result>
		</action>
	
		<action name="*ActSupportAction" class="com.cct.labstruts2.web.actionsupport.action.LoginActionSupportAction" method="{1}">
			<result name="login">/jsp/lab/actionsupport/action/login.jsp</result>
			<result name="success">/jsp/lab/actionsupport/action/loginSuccess.jsp</result>
			<result name="error">/jsp/lab/actionsupport/action/loginFail.jsp</result>
		</action>
		
	</package>
</struts>